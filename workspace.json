    "home": {
      "root": "apps/home",
      "sourceRoot": "apps/home",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "root": "apps/home",
            "outputPath": "dist/apps/home",
            "assets": [
              {
                "glob": "libs/assets/src",
                "input": ".",
                "output": "."
              }
            ]
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "home:build",
            "dev": true,
            "proxyConfig": "apps/home/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "home:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "home:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/home"
          ],
          "options": {
            "jestConfig": "apps/home/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/home/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    },
