{
  "version": 2,
  "projects": {
    "atoms": {
      "root": "libs/atoms",
      "sourceRoot": "libs/atoms/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/atoms",
            "tsConfig": "libs/atoms/tsconfig.lib.json",
            "project": "libs/atoms/package.json",
            "entryFile": "libs/atoms/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/atoms/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/atoms/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/atoms"],
          "options": {
            "jestConfig": "libs/atoms/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "gatekeeper": {
      "root": "apps/gatekeeper",
      "sourceRoot": "apps/gatekeeper",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/gatekeeper",
            "outputPath": "dist/apps/gatekeeper"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "gatekeeper:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "gatekeeper:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "gatekeeper:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/gatekeeper"],
          "options": {
            "jestConfig": "apps/gatekeeper/jest.config.js",
            "passWithNoTests": true,
            "codeCoverage": true,
            "colors": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/gatekeeper/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "run-coveralls": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": ["coverage/lcov.info"],
          "options": {
            "command": "yarn jest --coverage && coveralls < coverage/lcov.info"
          }
        }
      }
    },
    "gatekeeper-e2e": {
      "root": "apps/gatekeeper-e2e",
      "sourceRoot": "apps/gatekeeper-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/gatekeeper-e2e/cypress.json",
            "tsConfig": "apps/gatekeeper-e2e/tsconfig.e2e.json",
            "devServerTarget": "gatekeeper:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gatekeeper:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/gatekeeper-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "gfx": {
      "root": "libs/gfx",
      "sourceRoot": "libs/gfx/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/gfx",
            "tsConfig": "libs/gfx/tsconfig.lib.json",
            "project": "libs/gfx/package.json",
            "entryFile": "libs/gfx/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/gfx/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/gfx/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/gfx"],
          "options": {
            "jestConfig": "libs/gfx/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "layout": {
      "root": "libs/layout",
      "sourceRoot": "libs/layout/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/layout",
            "tsConfig": "libs/layout/tsconfig.lib.json",
            "project": "libs/layout/package.json",
            "entryFile": "libs/layout/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/layout/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/layout/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/layout"],
          "options": {
            "jestConfig": "libs/layout/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/utils",
            "tsConfig": "libs/utils/tsconfig.lib.json",
            "project": "libs/utils/package.json",
            "entryFile": "libs/utils/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/utils/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/utils/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils"],
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "gatekeeper"
}
