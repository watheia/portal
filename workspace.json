{
  "version": 2,
  "projects": {
    "gatekeeper": {
      "root": "apps/gatekeeper",
      "sourceRoot": "apps/gatekeeper",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "root": "apps/gatekeeper",
            "outputPath": "dist/apps/gatekeeper"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "gatekeeper:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "gatekeeper:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "gatekeeper:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/gatekeeper"
          ],
          "options": {
            "jestConfig": "apps/gatekeeper/jest.config.js",
            "passWithNoTests": true,
            "codeCoverage": true,
            "colors": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/gatekeeper/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "run-coveralls": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "coverage/lcov.info"
          ],
          "options": {
            "command": "yarn jest --coverage && coveralls < coverage/lcov.info"
          }
        }
      }
    },
    "gatekeeper-e2e": {
      "root": "apps/gatekeeper-e2e",
      "sourceRoot": "apps/gatekeeper-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/gatekeeper-e2e/cypress.json",
            "tsConfig": "apps/gatekeeper-e2e/tsconfig.e2e.json",
            "devServerTarget": "gatekeeper:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gatekeeper:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/gatekeeper-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "gatekeeper"
}
