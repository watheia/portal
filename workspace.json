{
  "version": 2,
  "projects": {
    "assets": {
      "root": "libs/assets",
      "sourceRoot": "libs/assets/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/assets/**/*.ts"]
          }
        }
      }
    },
    "atoms": {
      "root": "libs/atoms",
      "sourceRoot": "libs/atoms/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/atoms",
            "tsConfig": "libs/atoms/tsconfig.lib.json",
            "project": "libs/atoms/package.json",
            "entryFile": "libs/atoms/src/index.ts",
            "external": [
              "next/router",
              "next/link",
              "react/jsx-runtime",
              "react",
              "react-dom",
              "react-is",
              "@material-ui/core"
            ],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/atoms/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/atoms/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/atoms"],
          "options": {
            "jestConfig": "libs/atoms/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "context": {
      "root": "libs/context",
      "sourceRoot": "libs/context/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/context",
            "tsConfig": "libs/context/tsconfig.lib.json",
            "project": "libs/context/package.json",
            "entryFile": "libs/context/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/context/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/context/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/context"],
          "options": {
            "jestConfig": "libs/context/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "gatekeeper": {
      "root": "apps/gatekeeper",
      "sourceRoot": "apps/gatekeeper",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/gatekeeper",
            "outputPath": "dist/apps/gatekeeper"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "gatekeeper:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "gatekeeper:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "gatekeeper:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/gatekeeper"],
          "options": {
            "jestConfig": "apps/gatekeeper/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/gatekeeper/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "gatekeeper-e2e": {
      "root": "apps/gatekeeper-e2e",
      "sourceRoot": "apps/gatekeeper-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/gatekeeper-e2e/cypress.json",
            "devServerTarget": "gatekeeper:serve",
            "tsConfig": "apps/gatekeeper-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gatekeeper:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/gatekeeper-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "gfx": {
      "root": "libs/gfx",
      "sourceRoot": "libs/gfx/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/gfx",
            "tsConfig": "libs/gfx/tsconfig.lib.json",
            "project": "libs/gfx/package.json",
            "entryFile": "libs/gfx/src/index.ts",
            "external": ["react/jsx-runtime", "react", "react-dom", "react-is"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/gfx/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/gfx/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/gfx"],
          "options": {
            "jestConfig": "libs/gfx/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "home": {
      "root": "apps/home",
      "sourceRoot": "apps/home",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/home",
            "outputPath": "dist/apps/home"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "home:build",
            "dev": true,
            "port": 8080
          },
          "configurations": {
            "production": {
              "buildTarget": "home:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "home:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/home"],
          "options": {
            "jestConfig": "apps/home/jest.config.js",
            "passWithNoTests": true,
            "codeCoverage": true,
            "colors": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/home/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "run-coveralls": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": ["coverage/lcov.info"],
          "options": {
            "command": "yarn jest --coverage && coveralls < coverage/lcov.info"
          }
        },
        "dotdeps": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": ["dist/apps/home/dependencies.dot"],
          "options": {
            "command": "yarn depcruise --config .dependency-cruiser.js --output-type dot . | dot -T svg > depgraph.svg",
            "cwd": "apps/home"
          }
        }
      }
    },
    "home-e2e": {
      "root": "apps/home-e2e",
      "sourceRoot": "apps/home-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/home-e2e/cypress.json",
            "devServerTarget": "home:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "home:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/home-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
